{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": ["react-app", "airbnb", "plugin:jsx-a11y/recommended", "prettier", "prettier/react"],
  "plugins": ["jsx-a11y", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-console": "error",
    "react/jsx-props-no-spreading": "off"
  }
}
